{
    "root": true,
    "env": {
        "node": true
    },
    "extends": ["plugin:vue/vue3-recommended", "eslint:recommended", "plugin:prettier/recommended", "@vue/prettier"],
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["warn", "single", { "avoidEscape": true }],
        "no-console": "warn"
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "globals": {
        "wwLib": "readonly",
        "_": "readonly"
    }
}
